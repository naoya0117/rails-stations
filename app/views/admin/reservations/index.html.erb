<p id="notice"><%= notice %></p>
<table>
  <% @reservations.each do |reservation| %>
  <tr>
    <td><%= reservation.schedule.movie.name %></td>
    <td><%= "#{reservation.sheet.row}-#{reservation.sheet.column}" %></td>
    <td><%= "#{reservation.schedule.start_time.strftime('%Y-%m-%d %H:%M')}~#{reservation.schedule.end_time.strftime("%H:%M")}" %> </td>
    <td><%= link_to "編集", admin_reservation_path(reservation) %></td>
  </tr>
  <% end %>
</table>
<%= link_to "作成", new_admin_reservation_path %>
